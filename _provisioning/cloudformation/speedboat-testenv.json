    "TargetServer": {
      "Type": "AWS::EC2::Instance",
      "Properties": {
        "InstanceType": "m4.xlarge",
        "KeyName": "SpeedboatTest1",
        "ImageId": "ami-f95ef58a",
        "NetworkInterfaces": [
          {
            "GroupSet": [
              {
                "Ref": "SecurityGroup"
              }
            ],
            "AssociatePublicIpAddress": "true",
            "DeviceIndex": "0",
            "DeleteOnTermination": "true",
            "SubnetId": {
              "Ref": "ServerSubnet"
            }
          }
        ],
        "UserData": {
          "Fn::Base64": {
            "Fn::Join": [
              "",
              [
                "#!/bin/bash -xe\n",
                "apt-get update -y\n",
                "curl -o /tmp/bootstrap_salt.sh -L https://bootstrap.saltstack.com\n",
                "sh /tmp/bootstrap_salt.sh -A ",
                {
                  "Fn::GetAtt": [
                    "LoadgenServer",
                    "PrivateIp"
                  ]
                },
                " -i target\n",
                "\n"
              ]
            ]
          }
        }
      },
      "Metadata": {
