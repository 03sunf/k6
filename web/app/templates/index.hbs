<div class="row">
  <div class="col-md-6 col-md-push-6">
    <h4>Active VUs <small><a href="#">click to scale</a></small></h4>
    <div class="progress">
      <div class="progress-bar" role="progressbar" arial-valuenow="{{vusActive}}" aria-valuemin="0" aria-valuemax="{{vusMax}}" style="width: {{vusPercent}}%; white-space: nowrap;"><span style="padding-left: 15px;">{{vusLabel}}</span></div>
    </div>

    <ul class="list-group">
      {{#each metrics as |metric|}}
      <li class="list-group-item metrics clearfix">
        <span class="name">{{metric.id}}</span>
        <span class="results pull-right">
          {{#each-in metric.sample as |key value| }}
          <span class="key">{{key}}=</span><span class="value">{{metric-format value metric.contains}}</span>
          {{/each-in}}
        </span>
      </li>
      {{/each}}
    </ul>
  </div>
  <div class="col-md-6 col-md-pull-6">
    {{sb-group group=model.defaultGroup}}
  </div>
</div>
